      "targets": [
        {
          "query": "vehicle_id = \"${vehicle_id}\"\n\nfrom(bucket: \"telemetry\")\n  |> range(start: -120s)\n  |> filter(fn: (r) =>\n    r[\"vehicle_id\"] == vehicle_id and\n    (\n      (r._measurement == \"speed\" and r._field == \"speed\") or\n      (r._measurement == \"battery\" and (r._field == \"battery_level\" or r._field == \"temperature\")) or\n      (r._measurement == \"tires\" and r._field == \"front_tire_pressure\")\n    )\n  )\n  |> aggregateWindow(every: 120s, fn: count, createEmpty: true)\n  |> fill(column: \"_value\", value: 0)\n  |> map(fn: (r) => ({\n    _time: r._time,\n    _value: if r._value > 0 then 1 else 0,\n    _field: \"connectivity\",\n    vehicle_id: r.vehicle_id\n  }))\n  |> yield(name: \"status\")\n",
          "refId": "A"
        }
      ],
      "title": "Connectivity",
      "transformations": [
        {
          "id": "reduce",
          "options": {
            "includeTimeField": true,
            "labelsToFields": false,
            "mode": "reduceFields",
            "reducers": [
              "lastNotNull"
            ]
          }
        }
      ],
      "type": "stat"
