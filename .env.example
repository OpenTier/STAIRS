# Database configuration
DB_HOST=localhost            # Database host
DB_PORT=5432                 # Database port
DB_NAME=my_database          # Database name
DB_USER=my_user              # Database username
DB_PASSWORD=my_password      # Database password

# API configuration
API_PORT=3000                # Port the API server will run on
API_KEY=your_api_key_here    # API key for accessing external services

# Authentication
JWT_SECRET=your_jwt_secret   # Secret key for JWT token signing
OAUTH_CLIENT_ID=client_id    # OAuth client ID
OAUTH_CLIENT_SECRET=client_secret # OAuth client secret

# External service URLs
REDIS_URL=redis://localhost:6379 # URL for Redis service
INFLUXDB_URL=http://localhost:8086 # URL for InfluxDB service

# Feature flags
ENABLE_FEATURE_X=true         # Enable feature X if true
ENABLE_FEATURE_Y=false        # Enable feature Y if false
